  <template name="errorResponse" value="res.status(500).send({&#10;                code: 500,&#10;                notify: true,&#10;                type: 'warning',&#10;                banner: true,&#10;                bannerClass: 'alert alert-dismissible alert-warning',&#10;                msg: '',&#10;                reload: false,&#10;                disable: false,&#10;                redirect: false,&#10;                redirectPage: '/index.html'&#10;            });" description="universal node error resp" toReformat="true" toShortenFQNames="true">
