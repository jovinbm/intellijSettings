  <template name="promise_new" value="return new Promise(function(resolve, reject){&#10;$END$&#10;})" description="creates a new resolve reject promise" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
